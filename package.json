{
  "name": "pep",
  "version": "0.0.0",
  "type": "module",
  "description": "Use prisma with express.js and postgresql...",
  "scripts": {
    "build": "tsc",
    "knex:init": "knex init --cwd src -x ts",
    "make:migration": "NODE_OPTIONS='--no-warnings --loader ts-node/esm' knex migrate:make --cwd src -x ts",
    "make:seeder": "NODE_OPTIONS='--no-warnings --loader ts-node/esm' knex seed:make --cwd src -x ts",
    "migrate": "NODE_OPTIONS='--no-warnings --loader ts-node/esm' knex migrate:latest --cwd src",
    "rollback": "NODE_OPTIONS='--no-warnings --loader ts-node/esm' knex migrate:rollback --cwd src",
    "start:dev": "nodemon src/index.ts"
  },
  "keywords": [
    "Prisma",
    "Express",
    "PostgreSQL"
  ],
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "knex": "^3.1.0",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.11",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  }
}
