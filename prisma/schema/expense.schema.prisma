model Expense {
  id           String   @id @default(cuid())
  amount       Float
  description  String?
  category     String
  incurredBy   User     @relation(fields: [incurredById], references: [id])
  incurredById String
  group        Group?   @relation(fields: [groupId], references: [id])
  groupId      String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
